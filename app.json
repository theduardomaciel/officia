{
    "expo": {
        "name": "AndServ",
        "slug": "officia",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "platforms": [
            "android"
        ],
        "primaryColor": "#1E1E1E",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#1E1E1E"
        },
        "notification": {
            "icon": "./assets/notification.png",
            "iosDisplayInForeground": true
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/android/adaptive-icon.png",
                "backgroundColor": "#1E1E1E"
            },
            "permissions": [],
            "softwareKeyboardLayoutMode": "pan",
            "splash": {
                "backgroundColor": "#1E1E1E",
                "resizeMode": "cover",
                "mdpi": "./assets/android/splash/mdpi.png",
                "hdpi": "./assets/android/splash/hdpi.png",
                "xhdpi": "./assets/android/splash/xhdpi.png",
                "xxhdpi": "./assets/android/splash/xxhdpi.png",
                "xxxhdpi": "./assets/android/splash/xxxhdpi.png",
                "dark": {
                    "backgroundColor": "#1E1E1E",
                    "mdpi": "./assets/android/splash/mdpi.png",
                    "hdpi": "./assets/android/splash/hdpi.png",
                    "xhdpi": "./assets/android/splash/xhdpi.png",
                    "xxhdpi": "./assets/android/splash/xxhdpi.png",
                    "xxxhdpi": "./assets/android/splash/xxxhdpi.png"
                }
            },
            "package": "com.theduardomaciel.officia"
        },
        "androidStatusBar": {
            "translucent": true,
            "barStyle": "light-content",
            "backgroundColor": "#1E1E1E"
        },
        "plugins": [
            [
                "expo-navigation-bar",
                {
                    "position": "absolute",
                    "backgroundColor": "#1E1E1E"
                }
            ],
            [
                "./withAndroidSplashScreen.js"
            ]
        ],
        "scheme": "officia",
        "jsEngine": "hermes",
        "extra": {
            "expo-navigation-bar": {
                "position": "absolute",
                "backgroundColor": "#1E1E1E"
            },
            "eas": {
                "projectId": "9de284c1-eab2-4ed1-89d5-6b3cdb6e7597"
            }
        }
    }
}