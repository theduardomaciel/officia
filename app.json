{
    "expo": {
        "name": "officia",
        "slug": "officia",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "platforms": [
            "android"
        ],
        "primaryColor": "#1E1E1E",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#1E1E1E"
        },
        "notification": {
            "icon": "./assets/notification.png",
            "iosDisplayInForeground": true
        },
        "updates": {
            "fallbackToCacheTimeout": 0,
            "url": "https://u.expo.dev/9de284c1-eab2-4ed1-89d5-6b3cdb6e7597"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/android/adaptive-icon.png",
                "backgroundColor": "#1E1E1E"
            },
            "permissions": [],
            "softwareKeyboardLayoutMode": "pan",
            "splash": {
                "backgroundColor": "#1E1E1E",
                "resizeMode": "cover",
                "image": "./assets/splash.png"
            },
            "package": "com.theduardomaciel.officia",
            "runtimeVersion": "1.0.0"
        },
        "androidStatusBar": {
            "translucent": true,
            "barStyle": "light-content"
        },
        "plugins": [
            [
                "expo-navigation-bar",
                {
                    "position": "absolute"
                }
            ],
            [
                "./withAndroidSplashScreen.js"
            ],
            [
                "expo-image-picker",
                {
                    "photosPermission": "Permission to access documents to read PDF files",
                    "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera",
                    "//": "Disables the microphone permission",
                    "microphonePermission": false
                }
            ],
            [
                "expo-updates",
                {
                    "username": "meninocoiso"
                }
            ],
            "@notifee/react-native"
        ],
        "scheme": "officia",
        "jsEngine": "hermes",
        "extra": {
            "expo-navigation-bar": {
                "position": "absolute"
            },
            "eas": {
                "projectId": "9de284c1-eab2-4ed1-89d5-6b3cdb6e7597"
            }
        },
        "ios": {
            "runtimeVersion": {
                "policy": "sdkVersion"
            }
        }
    }
}